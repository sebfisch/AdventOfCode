{
    "name": "advent-of-code",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "aoc",
    "workspaceFolder": "/home/me/code",
    "customizations": { 
      "vscode": {
        "settings": {
            "terminal.integrated.profiles.linux": {}
        },
        "extensions": [
            "mhutchie.git-graph",
            "vscjava.vscode-java-pack",
            "ziglang.vscode-zig"
        ]
      }
    },
    "remoteUser": "me"
}
